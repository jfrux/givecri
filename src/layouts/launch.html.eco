    <%- @getBlock('styles').add(["/vendor/twitter-bootstrap/css/bootstrap.css","/vendor/twitter-bootstrap/css/bootstrap-responsive.css","/styles/font-awesome.css","/styles/style.css","/styles/switcher.css"]).toHTML() %>
